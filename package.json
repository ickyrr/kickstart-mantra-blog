    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "chai": "^3.5.0",
    "classnames": "2.2.1",
    "domready": "^1.0.8",
    "enzyme": "^1.4.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.15.0",
    "formsy-react": "0.17.0",
    "formsy-react-components": "0.6.6",
    "lodash": "^3.10.1",
    "mantra-core": "^1.2.0",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-komposer": "^1.3.0",
    "react-mounter": "^1.0.0",
    "react-select": "1.0.0-beta8",
    "react-simple-di": "^1.0.1",
